{
  "name": "trivyoperator.dashboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run prebuild && ng serve",
    "build": "ng build",
    "build:sme": "ng build --source-map=true --named-chunks==true && source-map-explorer dist/**/*.js",
    "build:ssr": "ng run TrivyOperator.Dashboard:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prebuild": "node build/preBuild.js",
    "update-endpoints:local": "curl -L http://localhost:5032/swagger/v1/swagger.yaml -o backend-api.yaml",
    "prettier:cli": "prettier '**/*.{css,scss,md,yml,json,js,ts,xml,html}'",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint:cli": "eslint",
    "eslint:check": "npm run eslint:cli",
    "eslint:fix": "npm run eslint:cli -- --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.10",
    "@angular/common": "^18.2.10",
    "@angular/compiler": "^18.2.10",
    "@angular/core": "^18.2.10",
    "@angular/forms": "^18.2.10",
    "@angular/platform-browser": "^18.2.10",
    "@angular/platform-browser-dynamic": "^18.2.10",
    "@angular/platform-server": "^18.2.10",
    "@angular/router": "^18.2.10",
    "@fortawesome/angular-fontawesome": "^0.15.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@microsoft/signalr": "^8.0.7",
    "chart.js": "^4.4.6",
    "mermaid": "^11.4.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.11",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular-eslint/builder": "^18.4.0",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular-eslint/eslint-plugin-template": "^18.4.0",
    "@angular-eslint/schematics": "^18.4.0",
    "@angular-eslint/template-parser": "^18.4.0",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.10",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^22.8.7",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "eslint-plugin-html": "^8.1.2",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-openapi-gen": "^0.52.0",
    "prettier": "^3.3.3",
    "prettier-plugin-multiline-arrays": "^3.0.6",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "autoprefixer": "10.4.5",
    "webpack": "5.81.0"
  }
}
